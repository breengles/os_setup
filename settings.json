{
    "extensions.ignoreRecommendations": true,
    "redhat.telemetry.enabled": false,
    "files.autoSave": "onWindowChange",
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/.aim/*/**": true,
        "**/logs/*/**": true,
        "**/output/*/**": true
    },
    "settingsSync.ignoredSettings": [
        "-python.formatting.blackPath",
        "-window.zoomLevel"
    ],
    "yaml.format.printWidth": 120,
    "[yaml]": {
        "editor.tabSize": 2
    },
    "ruff.lint.args": [
        "--line-length=120"
    ],
    "ruff.importStrategy": "useBundled",
    "debug.onTaskErrors": "abort",
    "security.workspace.trust.untrustedFiles": "open",
    // "remote.SSH.localServerDownload": "always",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.pdf": "default"
    },
    "window.zoomLevel": 1.2,
    "window.openFilesInNewWindow": "on",
    "window.openFoldersInNewWindow": "on",
    // terminal
    "terminal.integrated.profiles.linux": {
        "bash": null,
        "tmux": {
            "path": "bash",
            "args": [
                "-c",
                "ses_id=$(tmux new -dPF '#{session_id}') && new_name=${PWD##*/}-${ses_id:1} && tmux rename ${new_name/[.:]/_} && tmux split-window -h && tmux attach"
            ],
            "icon": "terminal-tmux",
        },
    },
    "terminal.integrated.profiles.osx": {
        "bash": null,
        "tmux": {
            "path": "zsh",
            "args": [
                "-c",
                "ses_id=$(tmux new -dPF '#{session_id}') && new_name=${PWD##*/}-${ses_id:1} && tmux rename ${new_name/[.:\\ ]/_} && tmux split-window -h && tmux attach"
            ],
            "icon": "terminal-tmux",
        },
    },
    "terminal.integrated.defaultProfile.linux": "tmux",
    "terminal.integrated.defaultProfile.osx": "tmux",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "terminal.integrated.inheritEnv": true,
    // editor
    "editor.minimap.showSlider": "always",
    "editor.fontFamily": "'CaskaydiaCove Nerd Font', 'Cascadia Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 18,
    "editor.rulers": [
        120
    ],
    "editor.renderWhitespace": "selection",
    "editor.detectIndentation": false,
    "editor.fontLigatures": false,
    "editor.formatOnSave": true,
    "editor.wordWrap": "on",
    "editor.inlineSuggest.enabled": true,
    "editor.guides.bracketPairs": "active",
    "editor.tabSize": 4,
    "editor.unicodeHighlight.ambiguousCharacters": true,
    // explorer
    "explorer.autoReveal": false,
    "explorer.confirmDelete": false,
    "explorer.openEditors.visible": 10,
    "explorer.confirmDragAndDrop": false,
    // git
    "git.openRepositoryInParentFolders": "never",
    "git.ignoreMissingGitWarning": true,
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "git.allowForcePush": true,
    "git.confirmSync": false,
    "git.confirmForcePush": true,
    "diffEditor.ignoreTrimWhitespace": false,
    // python
    "python.analysis.typeCheckingMode": "off",
    "jupyter.interactiveWindow.textEditor.executeSelection": true,
    "python.analysis.diagnosticSeverityOverrides": {
        "reportMissingImports": "none"
    },
    "python.terminal.executeInFileDir": true,
    "black-formatter.args": [
        "--line-length",
        "120"
    ],
    "black-formatter.importStrategy": "useBundled",
    "notebook.formatOnSave.enabled": true,
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports.ruff": true,
            "source.fixAll": false
        },
        "editor.formatOnType": true
    },
    "jupyter.askForKernelRestart": false,
    "python.analysis.completeFunctionParens": true,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    // fortran
    "fortran.notifications.releaseNotes": false,
    "fortran.formatting.formatter": "Disabled",
    "fortran.fortls.notifyInit": true,
    "[FortranFreeForm]": {
        "editor.tabSize": 2
    },
    "[FortranFixedForm]": {
        "editor.tabSize": 2
    },
    "fortran.fortls.maxLineLength": 120,
    "fortran.linter.compilerPath": "gfortran",
    "fortran.linter.compiler": "gfortran",
    // latex
    "latex-workshop.latex.recipe.default": "lastUsed",
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.latex.autoClean.run": "onFailed",
    "latex-workshop.latex.recipes": [
        {
            "name": "lualatex x2",
            "tools": [
                "lualatex",
                "lualatex"
            ]
        },
        {
            "name": "lualatex->biber->lualatex x2",
            "tools": [
                "lualatex",
                "biber",
                "lualatex",
                "lualatex"
            ]
        },
        {
            "name": "xelatex->biber->xelatex x2",
            "tools": [
                "xelatex",
                "biber",
                "xelatex",
                "xelatex"
            ]
        },
        {
            "name": "pdflatex -> biber -> pdflatex x2",
            "tools": [
                "pdflatex",
                "biber",
                "pdflatex",
                "pdflatex"
            ]
        },
        {
            "name": "pdflatex -> bibtex -> pdflatex x2",
            "tools": [
                "pdflatex",
                "bibtex",
                "pdflatex",
                "pdflatex"
            ]
        },
        {
            "name": "latexmk",
            "tools": [
                "latexmk"
            ]
        }
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "lualatex",
            "command": "lualatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "%DOC%"
            ]
        },
        {
            "name": "xelatex",
            "command": "xelatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "%DOC%"
            ]
        },
        {
            "name": "biber",
            "command": "biber",
            "args": [
                "%DOCFILE%"
            ]
        },
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        }
    ],
    // extensions
    "better-comments.highlightPlainText": true,
