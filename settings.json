{
    "debug.onTaskErrors": "abort",
    "terminal.integrated.inheritEnv": true,
    "security.workspace.trust.untrustedFiles": "open",
    "remote.SSH.localServerDownload": "always",
    "files.autoSave": "onWindowChange",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.pdf": "default"
    },
    // editor
    "editor.fontFamily": "'CaskaydiaCove Nerd Font', 'Cascadia Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 18,
    "editor.rulers": [
        120
    ],
    "editor.renderWhitespace": "selection",
    "editor.detectIndentation": false,
    "editor.fontLigatures": false,
    "editor.formatOnSave": true,
    "editor.wordWrap": "on",
    "editor.inlineSuggest.enabled": true,
    "editor.guides.bracketPairs": "active",
    "editor.tabSize": 4,
    "editor.unicodeHighlight.ambiguousCharacters": true,
    // explorer
    "explorer.confirmDelete": false,
    "explorer.openEditors.visible": 10,
    "explorer.confirmDragAndDrop": false,
    // git
    "git.ignoreMissingGitWarning": true,
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "git.allowForcePush": true,
    "git.confirmSync": false,
    "git.confirmForcePush": true,
    "diffEditor.ignoreTrimWhitespace": false,
    // python
    "python.terminal.executeInFileDir": true,
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "-l 120"
    ],
    "python.linting.pylintArgs": [
        "--generate-members"
    ],
    "[python]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        },
        "editor.formatOnType": true
    },
    "isort.args": [
        "--profile",
        "black"
    ],
    "python.autoComplete.extraPaths": [
        "/home/<pc-name>/<ws-name>/devel/lib/python3/dist-packages",
        "/opt/ros/noetic/lib/python3/dist-packages"
    ],
    "python.analysis.extraPaths": [
        "/home/<pc-name>/<ws-name>/devel/lib/python3/dist-packages",
        "/opt/ros/noetic/lib/python3/dist-packages"
    ],
    "jupyter.askForKernelRestart": false,
    "python.analysis.completeFunctionParens": true,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    // fortran
    "[FortranFreeForm]": {
        "editor.tabSize": 4
    },
    "[FortranFixedForm]": {
        "editor.tabSize": 4
    },
    "fortran.fortls.maxLineLength": 120,
    "fortran.linter.compilerPath": "gfortran",
    "fortran.linter.compiler": "gfortran",
    // latex
    "latex-workshop.latex.recipe.default": "lastUsed",
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.latex.autoClean.run": "onFailed",
    "latex-workshop.latexindent.path": "/usr/local/bin/latexindent",
    "latex-workshop.latex.recipes": [
        {
            "name": "lualatex x2",
            "tools": [
                "lualatex",
                "lualatex"
            ]
        },
        {
            "name": "lualatex->biber->lualatex x2",
            "tools": [
                "lualatex",
                "biber",
                "lualatex",
                "lualatex"
            ]
        },
        {
            "name": "xelatex->biber->xelatex x2",
            "tools": [
                "xelatex",
                "biber",
                "xelatex",
                "xelatex"
            ]
        },
        {
            "name": "pdflatex -> biber -> pdflatex x2",
            "tools": [
                "pdflatex",
                "biber",
                "pdflatex",
                "pdflatex"
            ]
        },
        {
            "name": "pdflatex -> bibtex -> pdflatex x2",
            "tools": [
                "pdflatex",
                "bibtex",
                "pdflatex",
                "pdflatex"
            ]
        },
        {
            "name": "latexmk",
            "tools": [
                "latexmk"
            ]
        }
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "lualatex",
            "command": "lualatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "%DOC%"
            ]
        },
        {
            "name": "xelatex",
            "command": "xelatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "%DOC%"
            ]
        },
        {
            "name": "biber",
            "command": "biber",
            "args": [
                "%DOCFILE%"
            ]
        },
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        }
    ],
